{
  "title" : "Diablo",
  "version": "1.09",
  "saveDir": ".diablo",
  "refWindowSize": [640, 480],
  "minWindowSize": [640, 480],
  "windowSize": [800, 600],
  "keepAR": true,
  "stretchToFit": true,
  "smoothScreen": true,
  "load": "settings.json",
  "init": true,
  "mountFile": [
    { "file": "DIABDAT.MPQ", "append": true }
  ],
  "action": {
    "name": "if.fileExists",
    "param1": "ui_art/title.pcx",
    "then": { "name": "load", "file": "ui/loadMain.json" },
    "else": { "name": "load", "file": "ui/dataMissing.json" }
  }
}