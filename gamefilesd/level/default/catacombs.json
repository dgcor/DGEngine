{
  "action": {
    "name": "if.equal",
    "param1": "%game.hasPalette.level%",
    "param2": false,
    "then": {
      "name": "loadJson",
      "json": {
        "palette": {
          "id": "level",
          "file": "levels/l2data/l2.pal"
        }
      }
    }
  },
  "load": ["level/data/l2/l2s.json", "level"],
  "level": {
    "id": "level",
    "palette": "level",
    "file": "levels/l2data/l2.cel",
    "layers": [
      { "index": 0, "texturePack": "l2s" },
      { "index": 0, "texturePack": "l2s_2" }
    ],
    "outOfBoundsTile": 11,
    "til": "levels/l2data/l2.til",
    "min": "levels/l2data/l2.min",
    "minBlocks": 10,
    "sol": "levels/l2data/l2.sol",
    "defaultLight": 255
  }
}