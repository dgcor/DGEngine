{
  "load.closePanels": "ui/level/char/closePanels.json",
  "action": [
    { "name": "resource.add", "id": "userPanel" },
    { "name": "game.clearPlayingSounds" }
  ],
  "load.disableKeys": "ui/level/char/disableKeys.json",
  "inputEvent": [
    {
      "gameInputEvent": "esc",
      "action": { "name": "button.click", "id": "back", "playSound": false }
    },
    {
      "gameInputEvent": "enter",
      "action": "focus.click"
    },
    {
      "gameInputEvent": "up",
      "action": "focus.moveUp"
    },
    {
      "gameInputEvent": "down",
      "action": "focus.moveDown"
    }
  ],
  "load.smallPanel": "ui/level/panel/small.json"
}